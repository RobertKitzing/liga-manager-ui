    "scripts": {
        "copy:app-settings:local": "copy-files-from-to --mode app-local --silent",
        "copy:app-settings:prod": "copy-files-from-to --mode app-prod --silent"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/setup-jest.ts"
        ]
    },
    "copyFiles": [
        {
            "from": {
                "web": "appsettings/appsettings.web.json",
                "app-local": "appsettings/appsettings.app-local.json",
                "app-prod": "appsettings/appsettings.app-prod.json"
            },
            "to": {
                "web": "dist/apps/liga-manager-ui-web/browser/appsettings.json",
                "app-local": "dist/apps/liga-manager-ui-web/browser/appsettings.json",
                "app-prod": "dist/apps/liga-manager-ui-web/browser/appsettings.json"
            }
        }
    ],
    "copyFilesSettings": {
        "whenFileExists": "overwrite"
    },

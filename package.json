{
  "name": "liga-manager-ui",
  "version": "4.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host 0.0.0.0 --proxy-config proxy.conf.json --port 4200",
    "build": "rm -R dist && npm run build:web && npm run build:app && npm run build:server",
    "build:web": "ng build --aot --prod",
    "build:app": "ng build --aot --c app --output-path app/www --delete-output-path --base-href ./",
    "build:app:local": "ng build --aot -c app.local --output-path app/www --delete-output-path --base-href ./",
    "build:server:express": "rimraf server/express/dist && tsc -p server/express && cp server/express/package*.json server/express/dist && npm run build:web -- ----output-path server/express/dist/www",
    "build:server:graphql": "rimraf server/graphql/dist && tsc -p server/graphql && cp server/graphql/package*.json server/graphql/dist && cp server/graphql/schema.graphql server/graphql/dist",
    "docker:build": "npm run build:server:express && npm run build:web && docker build -t robertkitzing/liga-manager-ui:graphql . ",
    "docker:push": "docker push robertkitzing/liga-manager-ui:graphql",
    "cordova:build": "cd app && cordova build android",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "graphql-gen:api": "gql-gen --config codegenAPI.yml",
    "graphql-gen:node": "gql-gen --config codegenNODE.yml",
    "docker:deploy": "npm run docker:build && npm run docker:push",
    "extract:all": "ngx-translate-extract -m -i ./src/app/ -o ./src/app/services/i18n/de.json ./src/app/services/i18n/en.json --clean --sort --format namespaced-json -m",
    "extract:update": "ngx-translate-extract -m -i ./src/app -o ./src/app/services/i18n/*.json --clean --sort --format namespaced-json -m",
    "codacy:coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d docs"
  },
  "dependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.9",
    "@angular/compiler": "^7.2.9",
    "@angular/core": "^7.2.9",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.9",
    "@angular/http": "^7.2.9",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "^7.2.9",
    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/router": "^7.2.9",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/googlemaps": "^3.30.19",
    "adm-zip": ">=0.4.13",
    "apollo-angular": "^1.5.0",
    "apollo-angular-link-http": "^1.5.0",
    "apollo-cache-hermes": "^0.8.8",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.5.1",
    "apollo-link-context": "^1.0.17",
    "apollo-link-ws": "^1.0.17",
    "bootstrap": "^4.3.1",
    "codacy-coverage": "^3.4.0",
    "command-line-args": "^5.0.2",
    "cordova-set-version": "^7.0.4",
    "core-js": "^2.6.5",
    "exec": "^0.2.1",
    "graphql": "^14.1.1",
    "graphql-code-generator": "^0.18.0",
    "graphql-codegen-add": "^0.18.0",
    "graphql-codegen-fragment-matcher": "^0.18.0",
    "graphql-codegen-introspection": "^0.18.0",
    "graphql-codegen-typescript-apollo-angular": "^0.18.0",
    "graphql-codegen-typescript-client": "^0.18.0",
    "graphql-codegen-typescript-common": "^0.18.0",
    "graphql-codegen-typescript-server": "^0.18.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "js-base64": "^2.5.1",
    "jslint": "^0.12.1",
    "karma-firefox-launcher": "^1.1.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "ng-pick-datetime": "^7.0.0",
    "ngx-store": "^2.1.0",
    "nswag": "^11.20.1",
    "popper.js": "^1.14.7",
    "queueing-subject": "^0.3.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0",
    "rxjs-websockets": "^6.0.2",
    "subscriptions-transport-ws": "^0.9.16",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.6",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/language-service": "^7.2.9",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "~3.3.10",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.11.3",
    "@types/yargs": "^12.0.9",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.3.0",
    "ts-node": "~8.0.3",
    "tslint": "~5.14.0",
    "typescript": "^3.2.4"
  }
}

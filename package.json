{
  "name": "liga-manager-ui",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host '0.0.0.0' --port 3098 --disable-host-check --proxy-config proxy.conf.json",
    "build": "rm -R dist && npm run build:web && npm run build:app && npm run build:server",
    "build:angular:web": "ng build --aot --prod",
    "build:angular:app": "ng build --aot --c app --output-path app/www --delete-output-path --base-href ./",
    "build:app:local": "ng build --aot -c app.local --output-path app/www --delete-output-path --base-href ./",
    "build:server:express": "rimraf server/express/dist && tsc -p server/express && cp server/express/package*.json server/express/dist && npm run build:angular:web -- ----output-path server/express/dist/www",
    "build:server:graphql": "rimraf server/graphql/dist && tsc -p server/graphql && cp server/graphql/package*.json server/graphql/dist && cp server/graphql/schema.graphql server/graphql/dist",
    "docker:build:express": "docker build -t robertkitzing/liga-manager-ui:graphql server/express",
    "docker:push": "docker push robertkitzing/liga-manager-ui:graphql",
    "cordova:build": "cd app && cordova build android",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "graphql-gen:api": "gql-gen --config codegenAPI.yml",
    "graphql-gen:node": "gql-gen --config codegenNODE.yml",
    "docker:deploy": "npm run docker:build && npm run docker:push",
    "extract:all": "ngx-translate-extract -m -i ./src/app/ -o ./src/app/services/i18n/de.json ./src/app/services/i18n/en.json --clean --sort --format namespaced-json -m",
    "extract:update": "ngx-translate-extract -m -i ./src/app -o ./src/app/services/i18n/*.json --clean --sort --format namespaced-json -m",
    "codacy:coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d docs",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "dependencies": {
    "@angular/animations": "^9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.1",
    "@angular/compiler": "^9.0.1",
    "@angular/core": "^9.0.1",
    "@angular/forms": "^9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "@angular/router": "^9.0.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/googlemaps": "^3.37.7",
    "adm-zip": ">=0.4.13",
    "apollo-angular": "^1.7.0",
    "apollo-angular-link-http": "^1.8.0",
    "apollo-cache-hermes": "^0.8.9",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-ws": "^1.0.19",
    "bootstrap": "^4.3.1",
    "codacy-coverage": "^3.4.0",
    "command-line-args": "^5.1.1",
    "cordova-set-version": "^8.1.3",
    "core-js": "^3.3.2",
    "exec": "^0.2.1",
    "graphql": "^14.5.8",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-add": "^0.18.2",
    "graphql-codegen-fragment-matcher": "^0.18.2",
    "graphql-codegen-introspection": "^0.18.2",
    "graphql-codegen-typescript-apollo-angular": "^0.18.2",
    "graphql-codegen-typescript-client": "^0.18.2",
    "graphql-codegen-typescript-common": "^0.18.2",
    "graphql-codegen-typescript-server": "^0.18.2",
    "jquery": "^3.4.1",
    "js-base64": "^2.5.1",
    "jslint": "^0.12.1",
    "karma-firefox-launcher": "^1.2.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "ngx-store": "^2.1.0",
    "ngx-webstorage": "^5.0.0",
    "popper.js": "^1.15.0",
    "queueing-subject": "^0.3.4",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "rxjs-websockets": "^8.0.0",
    "subscriptions-transport-ws": "^0.9.16",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "^9.0.2",
    "@angular/compiler-cli": "^9.0.1",
    "@angular/language-service": "^9.0.1",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "@types/yargs": "^13.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "^3.7.5"
  }
}

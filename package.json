{
  "name": "liga-manager",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:app": "npm run build -- --prod --environment app",
    "build:deploy": "npm run build && npm run deploy",
    "build:docker": "npm run build -- --prod --environment prod && docker build -t 'robertkitzing/liga-manager-ui:0.3' .",
    "docker:push": "docker push robertkitzing/liga-manager-ui:0.3",
    "docker:deploy": "npm run build:docker && npm run docker:push",
    "app:gensplash": "splashicon-generator",
    "app:build":  "cordova build --device --debug",
    "start": "ng serve --aot --host 0.0.0.0 --proxy-config proxy.conf.json --watch",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    // "test": "ng test",
    // "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "cordova:prepare": "ngx-scripts cordova prepare",
    "cordova:run": "ngx-scripts cordova run",
    "cordova:build": "ngx-scripts cordova build --device --debug --copy dist",
    "cordova:clean": "ngx-scripts clean",
    "cordova": "cordova",
    "generate": "ng generate",
    "nswag": "nswag run ligamanagerapi.nswag",
    "deploy": "node deploy",
    "copy:app": "cpx platforms/android/app/build/outputs/apk/debug/*.apk dist",
    "splashicon-generator": "splashicon-generator"
  },
  "dependencies": {
    "@angular/animations": "^5.2.3",
    "@angular/cdk": "^5.0.0",
    "@angular/common": "^5.2.3",
    "@angular/compiler": "^5.2.3",
    "@angular/core": "^5.2.3",
    "@angular/flex-layout": "2.0.0-beta.12",
    "@angular/forms": "^5.2.3",
    "@angular/http": "^5.2.3",
    "@angular/material": "^5.0.0",
    "@angular/material-moment-adapter": "^5.2.4",
    "@angular/platform-browser": "^5.2.3",
    "@angular/platform-browser-dynamic": "^5.2.3",
    "@angular/router": "^5.2.3",
    "@angular/service-worker": "^5.2.3",
    "@ionic-native/core": "^4.0.0",
    "@ionic-native/keyboard": "^4.0.0",
    "@ionic-native/splash-screen": "^4.0.0",
    "@ionic-native/status-bar": "^4.0.0",
    "@ngx-translate/core": "^9.0.1",
    "@types/ace": "^0.0.37",
    "@types/googlemaps": "^3.30.8",
    "@types/marked": "^0.3.0",
    "@types/socket.io-client": "^1.4.32",
    "ace": "^1.3.0",
    "amazing-time-picker": "^1.5.3",
    "angular-font-awesome": "^3.1.2",
    "bootstrap": "^4.0.0",
    "cordova-android": "^7.0.0",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-device": "^2.0.1",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-splashscreen": "^5.0.1",
    "cordova-plugin-statusbar": "^2.4.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.5.2",
    "cpx": "^1.5.0",
    "font-awesome": "^4.7.0",
    "global": "^4.3.2",
    "hammerjs": "^2.0.8",
    "ionic-plugin-keyboard": "^2.2.1",
    "js-base64": "^2.4.3",
    "lodash": "^4.17.4",
    "marked": "^0.3.19",
    "material-design-icons": "^3.0.1",
    "ngx-bootstrap": "^2.0.3",
    "ngx-markdown": "^1.5.2",
    "ngx-markdown-editor": "^1.1.7",
    "nswag": "^11.17.0",
    "queueing-subject": "^0.2.0",
    "rxjs": "^5.5.5",
    "rxjs-websockets": "^4.0.0",
    "splashicon-generator": "^0.2.12",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "~1.6.5",
    "@angular/compiler-cli": "^5.2.3",
    "@angular/language-service": "^5.2.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@ngx-rocket/scripts": "^2.0.0",
    "@types/jasmine": "^2.5.52",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.52",
    "@types/node": "^8.0.10",
    "codelyzer": "^4.1.0",
    "cordova": "^8.0.0",
    "ftp-deploy": "^1.2.2",
    "graceful-fs": "^4.1.11",
    "hads": "^1.3.4",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.2.2",
    "puppeteer": "^1.0.0",
    "rimraf": "^2.6.2",
    "stylelint": "~8.4.0",
    "stylelint-config-recommended-scss": "~3.0.0",
    "stylelint-config-standard": "~18.0.0",
    "stylelint-scss": "~2.2.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-ionic-webview": {}
    }
  }
}
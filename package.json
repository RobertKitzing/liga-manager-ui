{
  "name": "liga-manager-ui",
  "version": "5.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host '0.0.0.0' --port 3098 --disable-host-check --proxy-config proxy.conf.json --poll 1000",
    "build": "rm -R dist && npm run build:web && npm run build:app && npm run build:server",
    "build:angular:web": "ng build --aot --prod",
    "build:angular:app": "ng build --aot --c app --output-path app/www --delete-output-path --base-href ./ && npm run copy:appsettings:app",
    "build:app:local": "ng build --aot -c app.local --output-path app/www --delete-output-path --base-href ./ && npm run copy:appsettings:app:local",
    "build:server:express": "rimraf server/express/dist && tsc -p server/express && cp server/express/package*.json server/express/dist && npm run build:angular:web -- ----output-path server/express/dist/www",
    "build:server:graphql": "rimraf server/graphql/dist && tsc -p server/graphql && cp server/graphql/package*.json server/graphql/dist && cp server/graphql/schema.graphql server/graphql/dist",
    "docker:build:express": "docker build -t robertkitzing/liga-manager-ui:graphql server/express",
    "docker:push": "docker push robertkitzing/liga-manager-ui:graphql",
    "cordova:build": "cd app && cordova build android",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "graphql-gen:api": "graphql-codegen --config codegenAPI.yml",
    "graphql-gen:node": "gql-gen --config codegenNODE.yml",
    "docker:deploy": "npm run docker:build && npm run docker:push",
    "extract:all": "ngx-translate-extract -m -i ./src/app/ -o ./src/app/services/i18n/de.json ./src/app/services/i18n/en.json --clean --sort --format namespaced-json -m",
    "extract:update": "ngx-translate-extract -m -i ./src/app -o ./src/app/services/i18n/*.json --clean --sort --format namespaced-json -m",
    "codacy:coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d docs",
    "copy:appsettings:app": "cp src/appsettings.app.json app/www/appsettings.json",
    "copy:appsettings:app:local": "cp src/appsettings.app.local.json app/www/appsettings.json"
  },
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@fullcalendar/angular": "^5.10.1",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/list": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript-apollo-angular": "^3.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/googlemaps": "^3.37.7",
    "adm-zip": ">=0.4.13",
    "apollo-angular": "^2.6.0",
    "apollo-cache-hermes": "^0.8.9",
    "apollo-cache-persist": "^0.1.1",
    "bootstrap": "^4.3.1",
    "command-line-args": "^5.1.1",
    "cordova-set-version": "^8.1.3",
    "core-js": "^3.3.2",
    "exec": "^0.2.1",
    "graphql": "^15.0.0",
    "jquery": "^3.4.1",
    "js-base64": "^2.5.1",
    "jslint": "^0.12.1",
    "karma-firefox-launcher": "^1.2.0",
    "material-design-icons": "^3.0.1",
    "ngx-webstorage": "^9.0.0",
    "popper.js": "^1.15.0",
    "queueing-subject": "^0.3.4",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "rxjs-websockets": "^8.0.0",
    "subscriptions-transport-ws": "^0.9.16",
    "tslib": "^2.0.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.10.2",
    "@apollo/client": "^3.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.15",
    "@angular/cli": "^11.2.15",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "@types/yargs": "^13.0.3",
    "codelyzer": "^6.0.0",
    "dayjs": "^1.10.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0",
    "typescript": "^4.1.6"
  }
}
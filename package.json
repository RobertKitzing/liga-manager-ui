{
  "name": "liga-manager-ui",
  "version": "3.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host 0.0.0.0 --proxy-config proxy.conf.json --port 4200",
    "build": "rm -R dist && npm run build:web && npm run build:app && npm run build:server",
    "build:web": "ng build --aot --prod --output-path dist/server/src/www",
    "build:app": "ng build --aot -c app --output-path app/www --delete-output-path --base-href ./",
    "build:app:local": "ng build --aot -c app.local --output-path app/www --delete-output-path --base-href ./",
    "build:server": "tsc -p server --outDir dist",
    "docker:build": "npm run build:web && docker build -t robertkitzing/liga-manager-ui:add-match-day-entity . ",
    "docker:push": "docker push robertkitzing/liga-manager-ui:add-match-day-entity",
    "cordova:build": "cd app && cordova build android",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "nswag": "nswag run liga-manager-api.nswag",
    "docker:deploy": "npm run docker:build && npm run docker:push",
    "extract:all": "ngx-translate-extract -m -i ./src/app/ -o ./src/app/services/i18n/de.json ./src/app/services/i18n/en.json --clean --sort --format namespaced-json -m",
    "extract:update": "ngx-translate-extract -m -i ./src/app -o ./src/app/services/i18n/*.json --clean --sort --format namespaced-json -m"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/cdk": "^6.3.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.3.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/googlemaps": "^3.30.11",
    "bootstrap": "^4.1.2",
    "command-line-args": "^5.0.2",
    "cordova-set-version": "^7.0.3",
    "core-js": "^2.5.4",
    "d3": "^5.5.0",
    "exec": "^0.2.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "js-base64": "^2.4.8",
    "karma-firefox-launcher": "^1.1.0",
    "material-design-icons": "^3.0.1",
    "ng-pick-datetime": "^6.0.10",
    "nswag": "^11.20.1",
    "queueing-subject": "^0.3.0",
    "rxjs": "^6.0.0",
    "rxjs-websockets": "^6.0.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/d3": "^5.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/yargs": "^11.1.1",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}

{
  "name": "liga-manager-ui",
  "version": "3.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host 0.0.0.0 --proxy-config proxy.conf.json --port 4200",
    "build": "rm -R dist && npm run build:web && npm run build:app && npm run build:server",
    "build:web": "ng build --aot --prod --output-path dist/server/src/www",
    "build:app": "ng build --aot --c app --output-path app/www --delete-output-path --base-href ./",
    "build:app:local": "ng build --aot -c app.local --output-path app/www --delete-output-path --base-href ./",
    "build:server": "tsc -p server --outDir dist",
    "docker:build": "npm run build:web && docker build -t robertkitzing/liga-manager-ui:add-match-day-entity . ",
    "docker:push": "docker push robertkitzing/liga-manager-ui:add-match-day-entity",
    "cordova:build": "cd app && cordova build android",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "nswag": "nswag run liga-manager-api.nswag",
    "graphql-gen": "gql-gen --config codegen.yml",
    "docker:deploy": "npm run docker:build && npm run docker:push",
    "extract:all": "ngx-translate-extract -m -i ./src/app/ -o ./src/app/services/i18n/de.json ./src/app/services/i18n/en.json --clean --sort --format namespaced-json -m",
    "extract:update": "ngx-translate-extract -m -i ./src/app -o ./src/app/services/i18n/*.json --clean --sort --format namespaced-json -m",
    "codacy:coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d docs"
  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/googlemaps": "^3.30.15",
    "adm-zip": ">=0.4.11",
    "apollo-angular": "^1.5.0",
    "apollo-angular-link-http": "^1.4.0",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link-context": "^1.0.12",
    "bootstrap": "^4.1.2",
    "codacy-coverage": "^3.2.0",
    "command-line-args": "^5.0.2",
    "cordova-set-version": "^7.0.3",
    "core-js": "^2.5.4",
    "exec": "^0.2.1",
    "graphql": "^14.1.1",
    "graphql-code-generator": "^0.16.0",
    "graphql-codegen-fragment-matcher": "^0.16.0",
    "graphql-codegen-introspection": "^0.16.0",
    "graphql-codegen-typescript-apollo-angular": "^0.16.0",
    "graphql-codegen-typescript-client": "^0.16.0",
    "graphql-codegen-typescript-common": "^0.16.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "js-base64": "^2.4.8",
    "jslint": "^0.12.0",
    "karma-firefox-launcher": "^1.1.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.23.0",
    "ng-pick-datetime": "^6.0.16",
    "nswag": "^11.20.1",
    "popper.js": "^1.14.4",
    "queueing-subject": "^0.3.0",
    "rxjs": "^6.3.3",
    "rxjs-websockets": "^6.0.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~7.0.1",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@compodoc/compodoc": "^1.1.5",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/yargs": "^11.1.1",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3"
  }
}

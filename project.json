{
  "name": "liga-manager-ui",
  "$schema": "./node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": [
    "liga-manager-ui-web"
  ],
  "targets": {
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "defaultConfiguration": "local",
      "options": {
        "engine": "docker",
        "context": ".",
        "file": "Dockerfile"
      },
      "configurations": {
        "local": {
          "tags": [
            "liga-manager-ui-web:local"
          ],
          "push": false
        }
      }
    }
  }
}
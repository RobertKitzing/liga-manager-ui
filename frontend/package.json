{
  "name": "liga-manager-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --host '0.0.0.0' --proxy-config proxy.conf.json",
    "build:web": "ng build -c web",
    "build:app": "ng build -c app && npx cap sync",
    "build:web:stats": "ng build -c web --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "graphql-gen:api": "graphql-codegen --config codegenAPI.yml",
    "openapi-gen:api": "rm -R src/api/openapi && openapi-generator-cli generate -i http://localhost:3098/api-json -g typescript-angular -o src/api/openapi",
    "i18n:extract": "ngx-translate-extract --input ./src --output src/i18n/en-GB.json --clean --sort --format json",
    "i18n:languages": "node get-languages.mjs",
    "analyze": "npx webpack-bundle-analyzer dist/liga-manager-ui/stats.json",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.1",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.1",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@apollo/client": "^3.7.2",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@capacitor/android": "^4.6.2",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/core": "latest",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/typescript-apollo-angular": "^3.5.6",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/uuid": "^9.0.0",
    "apollo-angular": "^4.1.1",
    "cordova-res": "^0.15.4",
    "graphql": "^16.6.0",
    "hammerjs": "^2.0.8",
    "js-base64": "^3.7.3",
    "languages": "^0.1.3",
    "ngx-webstorage": "^11.1.1",
    "rxjs": "~7.5.0",
    "tiny-json-http": "^7.4.2",
    "ts-password-generator": "^1.0.6",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "~15.0.2",
    "@angular/compiler-cli": "^15.0.0",
    "@bartholomej/ngx-translate-extract": "^8.0.2",
    "@capacitor/cli": "latest",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.5.1",
    "eslint": "^8.31.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.8.2"
  },
  "overrides": {
    "apollo-angular": {
      "@angular/core": "^15.0.0"
    }
  }
}

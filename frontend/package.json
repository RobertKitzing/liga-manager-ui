{
  "name": "liga-manager-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --host '0.0.0.0' --proxy-config proxy.conf.json",
    "build:web": "ng build -c web",
    "build:app": "ng build -c app && npx cap sync",
    "build:web:stats": "ng build -c web --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "graphql-gen:api": "graphql-codegen --config codegenAPI.yml",
    "i18n:extract": "ngx-translate-extract --input ./src --output src/i18n/en-GB.json --clean --sort --format json",
    "i18n:languages": "node get-languages.mjs",
    "analyze": "npx webpack-bundle-analyzer dist/liga-manager-ui/stats.json",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix "
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.2",
    "@angular/cdk": "^16.1.2",
    "@angular/common": "^16.1.2",
    "@angular/compiler": "^16.1.2",
    "@angular/core": "^16.1.2",
    "@angular/forms": "^16.1.2",
    "@angular/material": "^16.1.2",
    "@angular/platform-browser": "^16.1.2",
    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/router": "^16.1.2",
    "@apollo/client": "^3.7.16",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@capacitor/android": "^4.6.2",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/core": "latest",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/typescript-apollo-angular": "^3.5.6",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/uuid": "^9.0.0",
    "apollo-angular": "^5.0.0",
    "cordova-res": "^0.15.4",
    "graphql": "^16.7.1",
    "hammerjs": "^2.0.8",
    "js-base64": "^3.7.3",
    "languages": "^0.1.3",
    "ngx-webstorage": "^12.0.0",
    "rxjs": "~7.5.0",
    "tiny-json-http": "^7.4.2",
    "ts-password-generator": "^1.0.6",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.1",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.1",
    "@angular/compiler-cli": "^16.1.2",
    "@bartholomej/ngx-translate-extract": "^8.0.2",
    "@capacitor/cli": "latest",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@softarc/eslint-plugin-sheriff": "^0.11.0",
    "@softarc/sheriff-core": "^0.11.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.5.1",
    "eslint": "^8.39.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.1.1",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "typescript": "~5.1.3"
  }
}
